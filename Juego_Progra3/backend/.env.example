# Configuración del servidor
PORT=3001
NODE_ENV=development

# URL del frontend para CORS
FRONTEND_URL=http://localhost:3000

# Configuración de Supabase
SUPABASE_URL=tu_supabase_url_aqui
SUPABASE_SERVICE_KEY=tu_supabase_service_key_aqui

# Nota: El SERVICE_KEY es diferente al ANON_KEY
# El SERVICE_KEY tiene privilegios de administrador y debe mantenerse secreto
# Obtén estas credenciales desde tu panel de Supabase en Settings > API
